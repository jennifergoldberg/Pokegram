{
  "name": "pokegram",
  "version": "1.0.0",
  "description": "A Pokemon themed photo share app",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "watch-sass": "sass --watch sass/src:public/styles",
    "watch-nodemon": "nodemon server.js",
    "dev": "open http://localhost:4000/ && concurrently -n sass,server -c 'bgAqua.bold,bgPurple.bold' 'npm:watch-sass' 'npm:watch-nodemon'",
    "devw": "wsl-open http://localhost:4000/ && concurrently -n sass,server -c 'bgAqua.bold,bgPurple.bold' 'npm:watch-sass' 'npm:watch-nodemon'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jennifergoldberg/Pokegram.git"
  },
  "author": "Jennifer Goldberg and Gerald Tiamzon",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jennifergoldberg/Pokegram/issues"
  },
  "homepage": "https://github.com/jennifergoldberg/Pokegram#readme",
  "dependencies": {
    "bootstrap": "^5.0.2",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "method-override": "^3.0.0",
    "mongoose": "^5.13.5",
    "sass": "^1.37.0"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.12"
  }
}
